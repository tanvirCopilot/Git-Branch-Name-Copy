{
  "name": "git-branch-copy",
  "displayName": "Git Branch Copy",
  "description": "Copy git branch name with a single click",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/git-branch-copy"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-branch-copy.copyBranchName",
        "title": "Copy Git Branch Name"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
